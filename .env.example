# Beispiel f√ºr Umgebungsvariablen
# Kopiere diese Datei zu .env und passe die Werte an

# IB TWS Verbindung
IB_HOST=127.0.0.1
IB_PORT=7497
IB_CLIENT_ID=1

# Trading Modus (true/false)
IS_PAPER_TRADING=true

# Kapital & Risiko
ACCOUNT_SIZE=100000.0
MAX_RISK_PER_TRADE_PCT=0.01

# Watchlist (komma-separiert)
WATCHLIST=AAPL,MSFT,GOOGL,AMZN,TSLA

# Dry Run (true/false) - keine echten Orders
DRY_RUN=true

# Logging Level (DEBUG, INFO, WARNING, ERROR)
LOG_LEVEL=INFO
